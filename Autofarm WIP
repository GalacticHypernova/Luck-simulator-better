getgenv().settings={
   ["autoFarm"]=false,
   ["upgrade"]=false,
   ["prestige"]=false,
   ["tokenUps"]=false,
   ["magnet"]=false,
   ["research"]=false
}
function hideStats()
    spawn(function()
        game:GetService("ReplicatedStorage").RemoteFunctions.ChangeSetting:InvokeServer("ShowOthersStats")
    end)
end
function collectGui()
    spawn(function()
        while wait() do
            if not getgenv().settings["magnet"] then break end
            for  i=1,100000000000,1 do
                game:GetService("ReplicatedStorage").RemoteFunctions.CollectGuiItem:InvokeServer(i)
            end
        end
    end)
end
function research()
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("1")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A2")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A3")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A5")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A6")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A8")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A9")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B2")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B3")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B5")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B6")
        end
    end)
    spawn(function()
        while wait(3) do
            if not getgenv().settings["research"] then break end
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B8")
        end
    end)
end
function researchOneTime()
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A1")
    end)
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A4")
    end)
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("A7")
    end)
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B1")
    end)
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B4")
    end)
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("B7")
    end)
    spawn(function()
            game:GetService("ReplicatedStorage").RemoteFunctions.Research:InvokeServer("C1")
    end)
end
function tokenUps()
    spawn(function()
        while wait() do
            game:GetService("ReplicatedStorage").RemoteFunctions.TokenUpgrade:InvokeServer("Red",1)
        end
    end)  
    spawn(function()
        while wait() do
            game:GetService("ReplicatedStorage").RemoteFunctions.TokenUpgrade:InvokeServer("Red",2)
        end
    end) 
    spawn(function()
        while wait() do
            game:GetService("ReplicatedStorage").RemoteFunctions.TokenUpgrade:InvokeServer("Red",3)
        end
    end) 
    spawn(function()
        while wait() do
            game:GetService("ReplicatedStorage").RemoteFunctions.TokenUpgrade:InvokeServer("Yellow",1)
        end
    end) 
    spawn(function()
        while wait() do
            game:GetService("ReplicatedStorage").RemoteFunctions.TokenUpgrade:InvokeServer("Yellow",2)
        end
    end) 
    spawn(function()
        while wait() do
            game:GetService("ReplicatedStorage").RemoteFunctions.TokenUpgrade:InvokeServer("Yellow",3)
        end
    end) 
end
function prestige()
    spawn(function()
        game:GetService("ReplicatedStorage").RemoteFunctions.Prestige:InvokeServer()
    end)
end
function upgrade()
    spawn(function()
        while wait() do
            if not getgenv().settings["upgrade"] then break end
        game:GetService("ReplicatedStorage").RemoteFunctions.BasicUpgrade:InvokeServer("BetterLuck")
        end
    end)
    spawn(function()
        while wait() do
            if not getgenv().settings["upgrade"] then break end
        game:GetService("ReplicatedStorage").RemoteFunctions.BasicUpgrade:InvokeServer("FasterLuck")
        end
    end)
end
local library = loadstring(game:GetObjects("rbxassetid://7657867786")[1].Source)()
local Wait = library.subs.Wait -- Only returns if the GUI has not been terminated. For 'while Wait() do' loops

local PepsisWorld = library:CreateWindow({
Name = "Pepsi's World",
Themeable = {
Info = "Discord Server: VzYTJ7Y"
}
})

local GeneralTab = PepsisWorld:CreateTab({
Name = "General"
})
local FarmingSection = GeneralTab:CreateSection({
Name = "Autofarm"
})
local PetSection = GeneralTab:CreateSection({
Name = "Pets"
})
local Other = GeneralTab:CreateSection({
  Name = "Misc",
  Side="Right"
})
FarmingSection:AddToggle({
    Name="Auto Basic Upgrades (all)",
    Callback=function(bool)
        getgenv().settings["upgrade"]=bool
        if bool then
            upgrade()
        end
    end
})
FarmingSection:AddToggle({
    Name="Auto Token upgrades (all))",
    Callback=function(bool)
        getgenv().settings["tokenUps"]=bool
        if bool then
            tokenUps()
        end
    end
})
FarmingSection:AddButton({
    Name="Prestige",
    Callback=function()
        prestige()
    end
})
FarmingSection:AddToggle({
    Name="Auto collect GUI items",
    Callback=function(bool)
        getgenv().settings["magnet"]=bool
        if bool then
            collectGui()
        end
        end
})
FarmingSection:AddToggle({
    Name="Auto research",
    Callback=function(bool)
        getgenv().settings["research"]=bool
        if bool then
            research()
        end
        end
})
FarmingSection:AddButton({
    Name="Research one time upgrades",
    Callback=function()
        researchOneTime()
    end
})
Other:AddButton({
    Name="Hide others' stats (reduce lag)",
    Callback=function()
        hideStats()
    end
})
Other:AddSlider({
    Name="Walkspeed",
    Value=16,
    Min=16,
    Max=1000,
    Callback=function(value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=value
    end
})
Other:AddSlider({
    Name="Jump power",
    Value=50,
    Min=50,
    Max=1000,
    Callback=function(value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower=value
    end
})
